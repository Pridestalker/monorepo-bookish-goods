{
  "name": "elderbraum/bookish",
  "type": "wordpress-theme",
  "description": "Theme for bookish goods",
  "license": "proprietary",
  "require": {
    "php": ">7.4",
    "ext-json": "*",
    "aristath/kirki": "^3.0",
    "dusank/knapsack": "^10.0",
    "htmlburger/carbon-fields": "^3.1",
    "jjgrainger/posttypes": "^2.0",
    "monolog/monolog": "^2.0",
    "nesbot/carbon": "^2.29",
    "php-di/php-di": "^6.1",
    "symfony/google-mailer": "^5.2",
    "symfony/http-client": "^5.1",
    "symfony/http-foundation": "^5.0",
    "symfony/mailer": "^5.0",
    "symfony/mime": "^5.0",
    "timber/timber": "^1.18",
    "twig/cssinliner-extra": "^3.0",
    "vlucas/phpdotenv": "^5.3",
    "yahnis-elsts/plugin-update-checker": "^4.8.1"
  },
  "require-dev": {
    "johnpbloch/wordpress-core": "^5.6",
    "roave/security-advisories": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    },
    "files": [
      "vendor/aristath/kirki/kirki.php",
      "src/app.php"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "allow-plugins": {
      "composer/installers": true
    }
  },
  "scripts": {
    "wire-install": [
        "php -r \"copy('.env.example', '.env');\""
    ]
  }
}
